    <TextView
        android:id="@+id/tv_main_frag_3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        app:layout_constraintEnd_toEndOf="@+id/iv_main_frag_3"
        app:layout_constraintStart_toStartOf="@+id/iv_main_frag_3"
        app:layout_constraintTop_toBottomOf="@+id/iv_main_frag_3"
        tools:text="@string/image_title_3" />

    <TextView
        android:id="@+id/tv_main_frag_6"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        app:layout_constraintEnd_toEndOf="@+id/iv_main_frag_6"
        app:layout_constraintStart_toStartOf="@+id/iv_main_frag_6"
        app:layout_constraintTop_toBottomOf="@+id/iv_main_frag_6"
        tools:text="@string/image_title_6" />

    <TextView
        android:id="@+id/tv_main_frag_7"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
