        android:id="@+id/tv_main_frag_2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        app:layout_constraintEnd_toEndOf="@+id/iv_main_frag_2"
        app:layout_constraintStart_toStartOf="@+id/iv_main_frag_2"
        app:layout_constraintTop_toTopOf="@+id/tv_main_frag_1"
        tools:text="@string/image_title_2" />
